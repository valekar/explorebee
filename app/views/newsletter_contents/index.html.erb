<h1>Listing newsletter_contents</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @newsletter_contents.each do |newsletter_content| %>
      <tr>
        <td><%= newsletter_content.name %></td>
        <td><%= newsletter_content.description %></td>
        <td><%= link_to 'Show', newsletter_content %></td>
        <td><%= link_to 'Edit', edit_newsletter_content_path(newsletter_content) %></td>
        <td><%= link_to 'Destroy', newsletter_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Newsletter content', new_newsletter_content_path %>
